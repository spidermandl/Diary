<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="textview">
		<item name="android:textSize">14sp</item>
		<item name="android:textColor">#aaffff33</item>
		<item name="android:shadowColor">#f1f2f3</item>
		<item name="android:shadowRadius">2.0</item>
		<item name="android:typeface">sans</item>
		<item name="android:textStyle">italic</item>
	</style>
	<style name="edittext">
		<item name="android:textSize">12sp</item>
		<item name="android:textColor">#f1f2f3</item>
		<item name="android:background">@drawable/text9</item>
		<item name="android:fromAlpha">0.2</item>
		<item name="android:toAlpha">1.0</item>
		<item name="android:typeface">serif</item>
		<item name="android:textStyle">bold</item>
		<item name="android:gravity">center</item>
	</style>
	<style name="button">
		<item name="android:textSize">12sp</item>
		<item name="android:textColor">#aaffffff</item>
		<item name="android:background">@drawable/button9</item>
		<item name="android:typeface">sans</item>
		<item name="android:textStyle">bold</item>

	</style>
</resources>
